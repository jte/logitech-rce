;
; Firmware Upgrade Driver
;
[DFUBTUSB]
AddReg=DFUBTUSB.reg 
CopyFiles=DFUBTUSB.CopyFiles, DFUBTUSB.inf.CopyFiles

[DFUBTUSB.NT]
AddReg=DFUBTUSB.reg 
CopyFiles=DFUBTUSB.CopyFiles.NT, DFUBTUSB.inf.CopyFiles

[DFUBTUSB.NT.Services]
Addservice = DFUBTUSB, 0x00000002, DFUBTUSB.AddService

[DFUBTUSB.AddService]
DisplayName    = %DFUBTUSB.SvcDesc%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %10%\System32\Drivers\frmupgr.sys
LoadOrderGroup = Base

[DFUBTUSB.reg]
HKR,,DevLoader,,"*ntkern"
HKR,,NTMPDriver,,frmupgr.sys

[DFUBTUSB.NTx86.5.1]
AddReg=DFUBTUSB.reg
CopyFiles=DFUBTUSB.CopyFiles.NT, DFUBTUSB.inf.CopyFiles

[DFUBTUSB.NTx86.5.1.Services]
Addservice = DFUBTUSB, 0x00000002, DFUBTUSB.AddService

[DFUBTUSB.CopyFiles]
frmupgr.sys

[DFUBTUSB.CopyFiles.NT]
frmupgr.sys

[DFUBTUSB.inf.CopyFiles]
;frmupgr.inf

[DestinationDirs]
DefaultDestDir             =11
DFUBTUSB.CopyFiles         =11
DFUBTUSB.CopyFiles.NT      =12
DFUBTUSB.inf.CopyFiles     =10,INF
;BTW.NT.CopyFiles           =11

